<idea-plugin>
  <change-notes>
      Logger plugin for IntelliJ Ide.&lt;br&gt;
      &lt;em&gt;Collect file created, deleted, copied, moved&lt;/em&gt;
      &lt;em&gt;Collect lines added, removed, changed&lt;em&gt;
      &lt;em&gt;Collect comments added and removed&lt;em&gt;</change-notes>
  <idea-version since-build="191.6707" until-build="191.*"/>
  <id>org.jetbrains</id>
  <name>Logger Intellij</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="fulviomarcelli3f@gmail.com" url="">Fulvio Marcelli</vendor>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <group id="MyPlugin.SampleMenu" text="Logger IntelliJ" description="logger login">
      <add-to-group group-id="MainMenu" anchor="last"/>
      <action id="Myplugin.Textboxes" class="Logger" text="login" description="login to logger server"/>
    </group>
  </actions>
</idea-plugin>
