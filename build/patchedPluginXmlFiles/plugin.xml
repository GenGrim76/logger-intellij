<idea-plugin>
  <idea-version since-build="223.8617" until-build="223.*" />
  <id>org.jetbrains</id>
  <name>Logger Intellij</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="fulviomarcelli3f@gmail.com" url="">Fulvio Marcelli</vendor>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij" />
  <actions>
    <group id="MyPlugin.SampleMenu" text="Logger IntelliJ" description="logger login">
      <add-to-group group-id="MainMenu" anchor="last" />
      <action id="Myplugin.Textboxes" class="Logger" text="login" description="login to logger server" />
    </group>
  </actions>
</idea-plugin>
